{# empty Twig template #}
<form class="form-inline" role="form" action="{{ path("fos_user_security_check") }}" method="post">
    <div class="form-group">
        <input type="text" class="form-control round" id="username" name="_username" placeholder="Utilisateur" required="required" />
    </div>
    <div class="form-group">
        <input type="password" class="form-control round" id="password" name="_password" placeholder="Password" required="required" />
    </div>
    <input type="hidden" name="_csrf_token" value="{{ csrf_token('authenticate') }}" />
    <input type="submit" class="btn-sm btn-blue" id="_submit" name="_submit" value="Se connecter" />
</form>